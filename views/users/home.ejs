    <style>
        body {
            background-color: #f4f0e7;
        }

        .categories,
        .new-arrivals,
        .christmas-special,
        .discount-section {
            background-color: #dedad2;
        }

        .text-overlay {
            position: absolute;
            width: 100%;
            height: 100%;
            bottom: 0;
            left: 50%;
            transform: translateX(-50%);
            color: rgb(45, 37, 37);
            font-size: 24px;
            font-weight: bold;
            display: flex;
            align-items: flex-end;
            padding-left: 50px;
            padding-bottom: 5px;
        }

        .text-overlay-top {
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 50%;
            transform: translateX(-50%);
            color: rgb(123, 193, 207);
            font-size: 24px;
            font-weight: bold;
            display: flex;
            align-items: flex-start;
            padding-left: 50px;
            padding-top: 30px;
        }

        .con {
            position: relative;
            display: inline-block;
        }

        .con img {
            display: block;
            width: 100%;
        }

        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(20px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .animated-text {
            display: flex;
            flex-direction: column;
            align-items: flex-start;
        }

        .animated-text span {
            display: block;
            animation: fadeInUp 0.8s ease-out forwards;
        }

        .animated-text span:first-child {
            margin-bottom: -35px;
        }

        .animated-text span:last-child {
            margin-left: 100px;
            animation-delay: 0.3s;
        }

        .hero__title {
            width: 100%;
            transition: transform 0.3s ease;
        }

        .hero__title:hover {
            transform: scale(1.05);
            text-shadow: 0 0 15px rgba(255, 255, 255, 0.5);
        }

        .hero__title span {
            display: block;
        }

        .hero__title span:last-child {
            margin-left: 100px;
        }

        .text-with-border {
            -webkit-text-stroke: 1px #333;
            /* text-shadow: 
                2px 2px 0 #333,
                -2px -2px 0 #333,
                2px -2px 0 #333,
                -2px 2px 0 #333; */
            line-height: 1.5;
        }

        .text-with-border span {
            display: block;
            margin-bottom: 5px;
            color: rgb(105, 95, 95);
        }
    </style>
